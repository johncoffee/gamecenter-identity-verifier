{
  "name": "gamecenter-identity-verifier-js",
  "version": "1.0.0-rc.2",
  "description": "A small library to verify the identity of apple-gamecenter's local player for consuming it in node.js backend server. A rewrite to typescript of maeltm/node-gamecenter-identity-verifier",
  "main": "dist/cjs/main.js",
  "module": "dist/esm/main.mjs",
  "types": "dist/types",
  "exports": {
    ".": {
      "require": "./dist/cjs/main.js",
      "import": "./dist/esm/main.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maeltm/node-gamecenter-identity-verifier.git"
  },
  "scripts": {
    "test": "node --test --import nock test/**/*.test.ts",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig-commonjs.json",
    "build:esm": "tsc -p tsconfig-esmodule.json",
    "postbuild:esm": "./rename-to-mjs.sh",
    "prepack": "npm run clean && npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "apple",
    "gamecenter",
    "pki",
    "crypto"
  ],
  "author": "johncoffee",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@types/node": "*",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "nock": "^14.0.10",
    "rimraf": "^6.1.0"
  },
  "files": [
    "dist"
  ]
}
